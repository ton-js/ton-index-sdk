{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@tsconfig/node-lts-strictest-esm/tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "resolveJsonModule": true
  },
  "files": [
    "webpack.config.ts"
  ],
  "ts-node": {
    "esm": true,
    "moduleTypes": {
      "webpack.config.ts": "cjs",
    }
  }
}
